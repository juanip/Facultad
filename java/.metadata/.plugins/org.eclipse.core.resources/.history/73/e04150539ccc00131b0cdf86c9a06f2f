public class PC {
	private int cores;
	private int ramGb;
	private String OS;
	
	public int getCores() {
		return cores;
	}
	
	public void setCores(int cores) {
		this.cores = cores;
	}
	
	public int getRamGb() {
		return ramGb;
	}
	
	public void setRamGb(int ramGb) {
		this.ramGb = ramGb;
	}
	
	public String getOS() {
		return OS;
	}
	
	public void setOS(String oS) {
		OS = oS;
	}
	
	
	
}
