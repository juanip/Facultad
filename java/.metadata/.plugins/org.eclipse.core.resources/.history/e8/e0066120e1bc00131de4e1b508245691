package clase3;

public class Ejercicio1 {
	private int[] numeros = new int[10];
	
	public void setArray(){
		carga();
	}
	
	public int[] getArray(){
		return this.numeros;
	}
	
	private void carga(){
		for(int i=0;i<10;i++){
			if(15+(i*3)%2==0){
				numeros[i] = (15+(i*3))/2;		
			}
			else{
				numeros[i] = 15+(i*3);
			}
		}
	}
	
	public int sumatoria(){
		int sumatoria = 0;
		for(int i=0;i<10;i++){
			sumatoria =+ numeros[i];
		}
		return sumatoria;
	}
	
}
