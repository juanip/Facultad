package manejoDeErrores;

import javax.swing.*;

public class Principal {
	public static void main(String[] args) {
		String rta = JOptionPane.showInputDialog(null, "Ingrese un n√∫mero", "Ejemplo TryCatch", JOptionPane.INFORMATION_MESSAGE);
		
		int nro = Integer.parseInt(rta);
		String mensaje;
		if(nro < 10){
			mensaje = "menor";
		}
		else if(nro > 10){
			mensaje = "mayor";
		}
		else{
			mensaje = "igual";
		}
		JOptionPane.showMessageDialog(null, );
		
		
	}

}
