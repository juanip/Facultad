package clase3;

public class Ejercicio1 {
	private int[] numeros = new int[10];
	
	public void setArray(){
		
	}
	
	public int[] getArray(){
		return this.numeros;
	}
	
	private void carga(){
		for(int i=0;i<10;i++){
			if(15+(i*3)%2==0){
				numeros[i] = (15+(i*3))/2;		
			}
			else{
				numeros[i] = 15+(i*3);
			}
		}
	}
	
}
